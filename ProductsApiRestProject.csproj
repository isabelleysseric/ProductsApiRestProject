<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework> <!-- Vous pouvez remplacer par net7.0 ou une autre version de .NET selon vos besoins -->
    <RootNamespace>ProductsApiRestProject</RootNamespace> <!-- Remplacez par le nom de votre projet -->
    <AssemblyName>ProductsApiRestProject</AssemblyName> <!-- Nom de l'assembly généré -->
    <OutputType>Exe</OutputType> <!-- Type de sortie, généralement Exe pour les projets API -->
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="8.0.0" /> <!-- Dépendance pour la gestion JSON -->
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.0" /> <!-- Dépendance pour Entity Framework avec SQL Server -->
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="8.0.0" /> <!-- Dépendance pour l'authentification JWT -->
  </ItemGroup>

  <!-- Ajoutez d'autres dépendances ici si nécessaire -->

</Project>

